<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="description" content="Gestaode escritorios de advogados ">
    <meta name="keywords" content="advogados, escritorios, sitema lawyerbox"
    <meta name="author" content="Franco Lucky Damiao">
   
    <title>@ViewData["Title"] - WeblawyersBox</title>

    <link href="~/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" />


    <link href="~/css/adminlte.css" rel="stylesheet" />



    <link href='https://cdn.jsdelivr.net/npm/fullcalendar@5.10.0/main.min.css' rel='stylesheet' />

    @*<link rel='stylesheet' href='https://fullcalendar.io/js/fullcalendar-3.1.0/fullcalendar.min.css' />*@
    <link href="~/lib/summernote/summernote-bs4.css" rel="stylesheet" />
    <link href="~/lib/select2/css/select2.css" rel="stylesheet" />
</head>
<body class="hold-transition dark-mode layout-top-nav">
    <header>
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-dark">
            <div class="container">
                <a href="#" class="navbar-brand">

                    <img src="~/images/seu-logo-aqui.png" alt="LawyerBox" class="brand-image img-circle elevation-0" width="150px" style="opacity: .8">
                    <span class="brand-text font-weight-light">LawyerBox</span>
                </a>

                <button class="navbar-toggler order-1" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse order-3" id="navbarCollapse">
                    <!-- Left navbar links -->
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-page="/processo/listarprocessos"><i class="fa fa-folder"></i> Processos</a>

                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-page="/calendario/index"><i class="fa fa-calendar"></i> Audiencias</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-page="/clientes/index"><i class="fa fa-user"></i> Clientes</a>
                        </li>

                    </ul>

                    <!-- SEARCH FORM -->
                    <form class="form-inline ml-0 ml-md-3">
                        <div class="input-group input-group-sm">
                            <input class="form-control form-control-navbar" type="search" placeholder="Search" aria-label="Search">
                            <div class="input-group-append">
                                <button class="btn btn-navbar" type="submit">
                                    <i class="fas fa-search"></i>
                                </button>
                            </div>
                        </div>
                    </form>
                </div>

                <!-- Right navbar links -->
                <ul class="order-1 order-md-3 navbar-nav navbar-no-expand ml-auto">
                    <!-- Messages Dropdown Menu -->
                    <partial name="_LoginPartial" />

                </ul>
            </div>
        </nav>
        <!-- /.navbar -->
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - WeblawyersBox - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/moment.js/moment.js"></script>
    @await Component.InvokeAsync("Notyf")
    <script src="~/lib/select2/js/select2.js"></script>
    <script src="~/lib/sweetalert/sweetalert.min.js"></script>
    <script src='https://cdn.jsdelivr.net/npm/fullcalendar@5.10.0/main.min.js'></script>
    <script src="https://cdn.datatables.net/1.11.3/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.3/js/dataTables.bootstrap4.min.js"></script>


    <script src="~/lib/summernote/summernote-bs4.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/Dialogbox.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
