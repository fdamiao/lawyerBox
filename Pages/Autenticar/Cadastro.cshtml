@page
@model WeblawyersBox.Pages.Autenticar.CadastroModel
@{
    Layout = null;
   // var photoPath = "~/images/" + "seu-logo-aqui.png";
}


<!DOCTYPE html>
<html lang="en">


<head>
    <title>Cadastro-LawyerBox</title>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="description" content="FLDservice-sistema de gestao de escritorios de advogados">
    <meta name="keywords" content=",Advogados,Adminstrativo,honorarios,timesheet,escritorios">
    <meta name="author" content="Franco lucky Damiao">
    <!-- Favicon icon -->
    <link rel="icon" href="~/assets/images/law.ico" type="image/x-icon">
    <!-- Google font-->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800" rel="stylesheet">
    <!-- Required Fremwork -->
   
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />

    <!-- themify-icons line icon -->
    <link rel="stylesheet" type="text/css" href="~/assets/icon/themify-icons/themify-icons.css">
    <!-- ico font -->
    <link rel="stylesheet" type="text/css" href="~/assets/icon/icofont/css/icofont.css">
    <!-- Style.css -->
    <link rel="stylesheet" type="text/css" href="~/assets/css/style.css">

</head>

<body class="horizontal-static">
    <section class="login p-fixed d-flex text-center bg-primary common-img-bg">
        <!-- Container-fluid starts -->
        <div class="container-fluid">
            <div class="row">
                <div class="col-sm-12">
                    <!-- Authentication card start -->
                    <div class="login-card card-block auth-body">
                        <form asp-route-returnUrl="@Model.ReturnUrl" class="md-float-material" method="post" enctype="multipart/form-data">

                            <div class="text-center">
                                <img src="~/images/LAWYERBOX.png" alt="logo.png">
                            </div>
                            <div class="auth-box">
                               
                                <p class="text-inverse b-b-default text-left "><a class="text-cyan" href="./index">Ja possui uma conta</a></p>
                                @*<div asp-validation-summary="All" class="text-danger"></div>*@
                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-group">

                                            <input asp-for="Input.nome" class="form-control" placeholder="Nome" />
                                            <span asp-validation-for="Input.nome" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">

                                            <input asp-for="Input.apelido" class="form-control" placeholder="Apelido" />
                                            <span asp-validation-for="Input.apelido" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">

                                    <input asp-for="Input.Email" class="form-control" placeholder="Email" />
                                    <span asp-validation-for="Input.Email" class="text-danger"></span>
                                </div>
                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-group">

                                            <input asp-for="Input.Password" class="form-control" placeholder="Palavra pass" />
                                            <span asp-validation-for="Input.Password" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">

                                            <input asp-for="Input.ConfirmPassword" class="form-control" placeholder="Confirmacao da palavra passe" />
                                            <span asp-validation-for="Input.ConfirmPassword" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">

                                    <input asp-for="Input.nomeescritorio" class="form-control" placeholder="Nome do escritorio" />
                                    <span asp-validation-for="Input.nomeescritorio" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                 

                                  
                                    <input type="file" asp-for="Input.Photo" aria-label="File browser" accept=".jpeg,.jpg" class="form-control">

                                    @*<input asp-for="Input.Photo" type="file" class="custom-file-input form-control" />*@
                                 


                                </div>
                             



                                <div class="row m-t-25 text-left">
                                    <div class="col-md-12">
                                        <div class="checkbox-fade fade-in-primary">
                                            <label>
                                                <input type="checkbox" value="">
                                                <span class="cr"><i class="cr-icon icofont icofont-ui-check txt-primary"></i></span>
                                                <span class="text-inverse">Aceitar  <a href="./termos">os Termos &amp; e Condiçoes.</a></span>
                                            </label>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="checkbox-fade fade-in-primary">
                                            <label>
                                                <input type="checkbox" value="">
                                                <span class="cr"><i class="cr-icon icofont icofont-ui-check txt-primary"></i></span>
                                                <span class="text-inverse">enviar <a href="#">Newsletter</a> semanalente.</span>
                                            </label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row m-t-30">
                                    <div class="col-md-12">
                                        <button type="submit" class="btn btn-primary btn-md btn-block waves-effect text-center m-b-20">Registar</button>

                                    </div>
                                </div>
                              
                            </div>
                        </form>
                        <!-- end of form -->
                    </div>
                    <!-- Authentication card end -->
                </div>
                <!-- end of col-sm-12 -->
            </div>
            <!-- end of row -->
        </div>
        <!-- end of container-fluid -->
    </section>

    <!-- Required Jquery -->
    <script type="text/javascript" src="~/lib/bower_components/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="~/lib./bower_components/jquery-ui/jquery-ui.min.js"></script>
    <script type="text/javascript" src="~/lib./bower_components/tether/dist/js/tether.min.js"></script>
    <script type="text/javascript" src="~/lib./bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- jquery slimscroll js -->
    <script type="text/javascript" src="~/lib./bower_components/jquery-slimscroll/jquery.slimscroll.js"></script>
    <!-- modernizr js -->
    <script type="text/javascript" src="~/lib./bower_components/modernizr/modernizr.js"></script>
    <script type="text/javascript" src="~/lib./bower_components/modernizr/feature-detects/css-scrollbars.js"></script>
    <!-- i18next.min.js -->
    <script type="text/javascript" src="~/lib./bower_components/i18next/i18next.min.js"></script>
    <script type="text/javascript" src="~/lib./bower_components/i18next-xhr-backend/i18nextXHRBackend.min.js"></script>
    <script type="text/javascript" src="~/lib./bower_components/i18next-browser-languagedetector/i18nextBrowserLanguageDetector.min.js"></script>
    <script type="text/javascript" src="~/lib./bower_components/jquery-i18next/jquery-i18next.min.js"></script>
    <!-- Custom js -->
    <script type="text/javascript" src="~/assets/js/script.js"></script>
   

    @section Scripts {
        <partial name="_ValidationScriptsPartial" />

        <script>
            $(document).ready(function () {
                debugger
                $('.custom-file-input').on("change", function () {
                    var fileName = $(this).val().split("\\").pop();
                    $(this).next('.custom-file-label').html(fileName);
                });
            });
        </script>
    }
</body>
</html>